<h1>お気に入りにした画像一覧</h1>
<tbody>
  <% @favorites.each do |favorite| %>
    <tr>
      <td><%= image_tag favorite.favorite_users %></td>
      <td><%= favorite.content %></td>
      <td><%= favorite.user.name %></td>
      <td><%= link_to '表示', picture %></td>
      <td><%= link_to '編集', edit_picture_path(picture) %></td>
      <td><%= link_to '削除', picture, method: :delete, data: { confirm: '削除しますか?' } %></td>
    </tr>
  <% end %>
</tbody>
